{
    "name": "algatux/clever",
    "description": "php cli command build with symfony and laravel components",
    "require": {
        "symfony/console": "^3.0",
        "symfony/finder": "^3.0",
        "fabpot/goutte": "^3.0",
        "illuminate/container": "^5.2",
        "illuminate/support": "^5.2",
        "illuminate/filesystem": "^5.2",
        "doctrine/orm": "^2.5",
        "doctrine/migrations": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Clever\\": "src/",
            "CleverTest\\": "test/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Galli",
            "email": "a.galli85@gmail.com"
        }
    ],
    "bin": ["src/bin/clever"],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
